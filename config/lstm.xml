<?xml version="1.0" encoding="utf-8" ?>
<config>
    <input_layer>
        <input_num>11</input_num>
    </input_layer>
    <output_layer>
        <output_num>2</output_num>
    </output_layer>
    <cell_layer>
        <time_step>22</time_step>
        <cell_unit>55</cell_unit>
    </cell_layer>
    <optimize>
        <batch_size>40</batch_size>
        <learn_rate>0.0009</learn_rate>
        <decay_rate>0.99</decay_rate>
        <moving_decay>0.99</moving_decay>
        <regularize_rate>0.0001</regularize_rate>
        <max_iter_times>2000</max_iter_times>
    </optimize>
    <mode>
        <mode_path>./mode_path/checkout</mode_path>
        <update_mode_freq>10</update_mode_freq>
    </mode>
</config>
